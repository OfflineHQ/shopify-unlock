{
  "name": "offline-unlock",
  "private": true,
  "scripts": {
    "build": "remix vite:build",
    "dev": "shopify app dev --config development",
    "config:link": "shopify app config link",
    "generate": "shopify app generate",
    "deploy": "shopify app deploy --config development",
    "config:use": "shopify app config use",
    "env": "shopify app env",
    "start": "remix-serve ./build/server/index.js",
    "docker-start": "npm run setup && npm run start",
    "setup": "prisma generate --schema=prisma/schema.prod.prisma && prisma migrate deploy",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "shopify": "shopify",
    "prisma": "prisma",
    "graphql-codegen": "graphql-codegen",
    "vite": "vite",
    "staging:dev": "shopify app dev --config staging",
    "production:dev": "shopify app dev --config production",
    "staging:deploy": "shopify app deploy --config staging",
    "production:deploy": "shopify app deploy --config production",
    "build:offline-connect:dev": "npm --prefix extensions/offline-connect-src run build:dev",
    "build:offline-connect:staging": "npm --prefix extensions/offline-connect-src run build:staging",
    "build:offline-connect:prod": "npm --prefix extensions/offline-connect-src run build:prod"
  },
  "type": "module",
  "engines": {
    "node": ">=21.6.2"
  },
  "dependencies": {
    "@prisma/client": "^5.15.0",
    "@remix-run/dev": "^2.7.1",
    "@remix-run/node": "^2.7.1",
    "@remix-run/react": "^2.7.1",
    "@remix-run/serve": "^2.7.1",
    "@shopify/app-bridge-react": "^4.1.2",
    "@shopify/polaris": "^12.0.0",
    "@shopify/polaris-icons": "^9.1.1",
    "@shopify/react-form": "^2.6.0",
    "@shopify/shopify-api": "^11.0.0",
    "@shopify/shopify-app-remix": "^3.0.0",
    "@shopify/shopify-app-session-storage-prisma": "^5.0.0",
    "isbot": "^5.1.0",
    "prisma": "^5.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-tsconfig-paths": "^4.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@remix-run/eslint-config": "^2.7.1",
    "@shopify/api-codegen-preset": "^1.0.0",
    "@types/eslint": "^8.40.0",
    "@types/node": "^20.6.3",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.4",
    "typescript": "^5.2.2",
    "vite": "^5.1.3"
  },
  "workspaces": {
    "packages": [
      "extensions/*"
    ]
  },
  "trustedDependencies": [
    "@shopify/plugin-cloudflare"
  ],
  "resolutions": {
    "undici": "6.13.0"
  },
  "overrides": {
    "undici": "6.13.0"
  },
  "author": "sebpalluel"
}